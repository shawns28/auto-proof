{
	"version": 1,
	"base_image": "codeocean/cuda-miniconda-jupyterlab:latest",
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"conda": {
			"packages": [
				{
					"name": "python",
					"version": "3.10.9"
				}
			]
		},
		"pip": {
			"packages": [
				{
					"name": "caveclient",
					"version": "5.30.1"
				},
				{
					"name": "cloud-volume",
					"version": "10.5.0"
				},
				{
					"name": "ipywidgets",
					"version": "8.1.5"
				},
				{
					"name": "jupyter",
					"version": "1.1.1"
				},
				{
					"name": "matplotlib",
					"version": "3.9.2"
				},
				{
					"name": "numpy",
					"version": "1.26.4"
				},
				{
					"name": "pandas",
					"version": "2.2.3"
				},
				{
					"name": "pathlib",
					"version": "1.0.1"
				},
				{
					"name": "tensorboard",
					"version": "2.18.0"
				},
				{
					"name": "torch",
					"version": "2.4.1"
				},
				{
					"name": "tqdm",
					"version": "4.66.5"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}